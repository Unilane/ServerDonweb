    createVendorOrderShipment(
        vendor_order_id: Int!,
        items: [VendorOrderItemInput]!,
        comment: String,
        tracking: [VendorOrderTrackingInput]
    ): CreateVendorOrderShipmentOutput @resolver(class: "\\Vnecoms\\VendorsSalesGraphQl\\Model\\Resolver\\CreateVendorOrderShipment") @doc(description:"Create order shipment")
