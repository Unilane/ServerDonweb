    ): VendorShipments! @doc(description: "The list of vendor order.") @resolver(class: "Vnecoms\\VendorsSalesGraphQl\\Model\\Resolver\\Shipment")

    vendorCreditmemos(
      filter: VendorCreditmemoFilterInput @doc(description: "Identifies which order attributes to search for and return."),
      pageSize: Int = 20 @doc(description: "Specifies the maximum number of results to return at once."),
      currentPage: Int = 1 @doc(description: "Specifies which page of results to return."),
      sort: VendorCreditmemoSortInput @doc(description: "Specifies which attributes to sort on, and whether to return the results in ascending or descending order.")
    ): VendorCreditmemos! @doc(description: "The list of vendor order.") @resolver(class: "Vnecoms\\VendorsSalesGraphQl\\Model\\Resolver\\Creditmemo")
}

type Mutation {
    createVendorOrderInvoice(
