<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Vnecoms\VendorsProductImportExport\Api\ImageRepositoryInterface" type="Vnecoms\VendorsProductImportExport\Model\ImageRepository" />
    <preference for="Vnecoms\VendorsProductImportExport\Api\Data\ImageInterface" type="Vnecoms\VendorsProductImportExport\Model\Data\Image" />
    <preference for="Vnecoms\VendorsProductImportExport\Api\Data\ImageSearchResultInterface"
                type="Magento\Framework\Api\SearchResults" />

    <preference for="Vnecoms\VendorsProductImportExport\Api\Data\DocumentContentInterface" type="Vnecoms\VendorsProductImportExport\Model\Data\DocumentContent" />
    <preference for="Vnecoms\VendorsProductImportExport\Api\Data\DocumentContentValidatorInterface" type="Vnecoms\VendorsProductImportExport\Model\Data\DocumentContentValidator" />
    <preference for="Vnecoms\VendorsProductImportExport\Api\Data\DocumentProcessorInterface" type="Vnecoms\VendorsProductImportExport\Model\Data\DocumentProcessor" />

    <type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
        <arguments>
            <argument name="collections" xsi:type="array">
                <item name="product_import_export_queue_data_source" xsi:type="string">Vnecoms\VendorsProductImportExport\Model\ResourceModel\Import\Grid\Collection</item>
            </argument>
        </arguments>
    </type>
    <type name="Vnecoms\VendorsProductImportExport\Helper\Data">
        <arguments>
            <argument name="notExportAttributes" xsi:type="array">
                <item name="approval" xsi:type="string">approval</item>
                <item name="vendor_id" xsi:type="string">vendor_id</item>
                <item name="credit_rate" xsi:type="string">credit_rate</item>
                <item name="credit_price" xsi:type="string">credit_price</item>
                <item name="credit_value_custom" xsi:type="string">credit_value_custom</item>
                <item name="credit_value_dropdown" xsi:type="string">credit_value_dropdown</item>
                <item name="credit_value_fixed" xsi:type="string">credit_value_fixed</item>
                <item name="credit_type" xsi:type="string">credit_type</item>
            </argument>
            <argument name="utf8Attributes" xsi:type="array">
                <item name="description" xsi:type="string">description</item>
                <item name="meta_description" xsi:type="string">meta_description</item>
                <item name="short_description" xsi:type="string">short_description</item>
            </argument>

        </arguments>
    </type>
</config>
