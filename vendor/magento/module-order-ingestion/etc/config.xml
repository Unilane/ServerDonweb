<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <offline_channel_payment>
                <active>1</active>
                <model>Magento\OrderIngestion\Model\Payment\Method</model>
                <order_status>pending</order_status>
                <title>Offline Channel Payment</title>
                <allowspecific>0</allowspecific>
                <group>offline</group>
            </offline_channel_payment>
        </payment>
        <carriers>
            <channelshipping>
                <active>0</active>
                <title>Channel</title>
                <name>Channel</name>
                <shipping_cost>0</shipping_cost>
                <sallowspecific>0</sallowspecific>
                <sort_order>15</sort_order>
                <model>Magento\OrderIngestion\Model\Carrier\Method</model>
            </channelshipping>
        </carriers>
    </default>
</config>
